{
    // DART formatting settings
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            120
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false,
        "editor.codeActionsOnSave": {
            "source.fixAll": true
        },
    },
    "editor.formatOnSave": true,
    "editor.wordWrapColumn": 120,
    "dart.lineLength": 120,
    "dart.debugSdkLibraries": false,
    "dart.showInspectorNotificationsForWidgetErrors": false,
    "dart.debugExternalPackageLibraries": false,
    // Other settings
    "files.exclude": {
        "ios/**/dgph": true,
        "ios/**/*.mode1v3": true,
        "ios/**/*.mode2v3": true,
        "ios/**/*.moved-aside": true,
        "ios/**/*.pbxuser": true,
        "ios/**/*.perspectivev3": true,
        "ios/**/*sync/": true,
        "ios/**/.sconsign.dblite": true,
        "ios/**/.tags*": true,
        "ios/**/.vagrant/": true,
        "ios/**/DerivedData/": true,
        "ios/**/Icon?": true,
        "ios/**/Pods/": true,
        "ios/**/.symlinks/": true,
        "ios/**/profile": true,
        "ios/**/xcuserdata": true,
        "ios/**/.generated/": true,
        "ios/Flutter/App.framework": true,
        "ios/Flutter/Flutter.framework": true,
        "ios/Flutter/Flutter.podspec": true,
        "ios/Flutter/Generated.xcconfig": true,
        "ios/**/Flutter/ephemeral/": true,
        "ios/Flutter/app.flx": true,
        "ios/Flutter/app.zip": true,
        "ios/**/Flutter/flutter_assets/": true,
        "ios/Flutter/flutter_export_environment.sh": true,
        "ios/**/ServiceDefinitions.json": true,
        "ios/Runner/GeneratedPluginRegistrant.*": true,
        "ios/**/default.mode1v3": false,
        "ios/**/default.mode2v3": false,
        "ios/**/default.pbxuser": false,
        "ios/**/default.perspectivev3": false,
        "**/*.class": true,
        "**/*.log": true,
        "**/*.pyc": true,
        "**/*.swp": true,
        "**/.DS_Store": true,
        "**/.atom/": true,
        "**/.buildlog/": true,
        "**/.history": true,
        "**/.svn/": true,
        "**/migrate_working_dir/": true,
        "**/*.iml": true,
        "**/*.ipr": true,
        "**/*.iws": true,
        "**/.idea/": true,
        "**/doc/api/": true,
        "**/ios/Flutter/.last_build_id": true,
        "**/.dart_tool/": true,
        "**/.flutter-plugins": true,
        "**/.flutter-plugins-dependencies": true,
        "**/.packages": true,
        "**/.pub-cache/": true,
        "**/.pub/": true,
        "build/": true,
        "lib/generated_plugin_registrant.dart": true,
        "**/app.*.symbols": true,
        "**/app.*.map.json": true,
        "android/app/debug": true,
        "android/app/profile": true,
        "android/app/release": true,
        "android/**/gradle-wrapper.jar": true,
        "android/.gradle": true,
        "android/captures/": true,
        "android/gradlew": true,
        "android/gradlew.bat": true,
        "android/local.properties": true,
        "android/**/GeneratedPluginRegistrant.java": true,
        "android/**/key.properties": true,
        "android/**/*.keystore": true,
        "android/**/*.jks": true
    },
    // Material Icons theme
    "material-icon-theme.folders.associations": {
        "features": "container",
    },
    // ToDo Highlight color settings
    "todohighlight.isEnable": true,
    "todohighlight.isCaseSensitive": true,
    "todohighlight.keywords": [
        {
            "text": " ToDo: ",
            "color": "#a8c023",
            "isWholeLine": true,
            "backgroundColor": "rgba(0,0,0,0)",
        },
        {
            "text": " Note: ",
            "color": "#05AAAE",
            "isWholeLine": true,
            "backgroundColor": "rgba(0,0,0,0)",
        },
        {
            "text": " Warning: ",
            "color": "#F27200",
            "isWholeLine": true,
            "backgroundColor": "rgba(0,0,0,0)",
        },
        {
            "text": " Title: ",
            "color": "#FF1200",
            "isWholeLine": true,
            "backgroundColor": "rgba(0,0,0,0)",
        },
        {
            "text": " Subtitle: ",
            "color": "#BE0011",
            "isWholeLine": true,
            "backgroundColor": "rgba(0,0,0,0)",
        }
    ],
}